{% extends 'base.html.twig' %}
{% set title = 'Case Studies' %}
{% block content %}
    <section class="case-studies">
        <div class="row small-up-1 medium-up-2 large-up-4">
            {% for case in cases %}
                <div class="column">
                    {{ case | raw }}
                </div>
            {% endfor %}
        </div>
        {% include 'panels/pagination.html.twig' %}
    </section>
{% endblock %}